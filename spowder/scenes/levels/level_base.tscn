@onready var anim_player : AnimationPlayer = $animation_player

@export_range(0.0, 1.0, 0.01, \"or_greater\", \"or_less\") var jitter_add : float = 0.0 :
	get: return RenderingServer.global_shader_parameter_get(&\"jitter_add\")
	set(value): RenderingServer.global_shader_parameter_set_override(&\"jitter_add\", value)


func _ready() -> void:
	anim_player.play(&\"fade_in\")
	
func _exit_tree() -> void:
	jitter_add = 0.0
"

